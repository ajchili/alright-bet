{
  "name": "alright-bet",
  "version": "0.0.1",
  "main": "src/server.tsx",
  "repository": "https://github.com/ajchili/alright-bet",
  "author": "Kirin Patel <kirinpatel@gmail.com>",
  "license": "MIT",
  "scripts": {
    "clear": "rm -rf ./dist",
    "lint": "tslint -c tslint.json -p tsconfig.server.json --fix",
    "build": "yarn clear && tsc --project tsconfig.server.json && yarn build:web",
    "build:web": "react-scripts build ",
    "dev": "nodemon --watch src -e ts,tsx --exec yarn dev:start",
    "dev:start": "ts-node --project tsconfig.server.json .",
    "start": "node ./dist/server.js"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "client-oauth2": "^4.3.0",
    "cookie-parser": "^1.4.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "jsonwebtoken": "^8.5.1",
    "node-fetch": "^2.6.0",
    "pg": "^8.2.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-scripts": "^3.4.1"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.6",
    "@types/express-session": "^1.17.0",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/node": "^14.0.1",
    "@types/node-fetch": "^2.5.7",
    "@types/pg": "^7.14.3",
    "@types/react-dom": "^16.9.8",
    "nodemon": "^2.0.3",
    "ts-node": "^8.10.1",
    "tsc": "^1.20150623.0",
    "tslint": "^6.1.2",
    "typescript": "^3.9.2"
  },
  "engines": {
    "node": "13.11.0"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
