{
  "name": "alright-bet",
  "version": "0.0.1",
  "main": "src/server.tsx",
  "repository": "https://github.com/ajchili/alright-bet",
  "author": "Kirin Patel <kirinpatel@gmail.com>",
  "license": "MIT",
  "proxy": "localhost:80",
  "scripts": {
    "clear": "rm -rf ./dist ./bundle",
    "lint": "tslint -c tslint.json -p tsconfig.server.json --fix",
    "prebuild": "yarn clear",
    "build": "yarn build:server && yarn build:web",
    "build:server": "tsc --project tsconfig.server.json",
    "build:web": "yarn webpack --progress",
    "dev:server": "nodemon --watch src -e ts,tsx --exec yarn dev:server:start",
    "dev:server:start": "ts-node --project tsconfig.server.json .",
    "dev:web": "yarn webpack --watch --progress",
    "start": "node ./dist/server.js"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "client-oauth2": "^4.3.0",
    "cookie-parser": "^1.4.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "jsonwebtoken": "^8.5.1",
    "node-fetch": "^2.6.0",
    "pg": "^8.2.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^3.4.1",
    "semantic-ui-react": "^0.88.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.6",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/preset-env": "^7.9.6",
    "@babel/preset-react": "^7.9.4",
    "@babel/preset-typescript": "^7.9.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.6",
    "@types/express-session": "^1.17.0",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/node": "^14.0.1",
    "@types/node-fetch": "^2.5.7",
    "@types/pg": "^7.14.3",
    "@types/react-dom": "^16.9.8",
    "@types/react-router-dom": "^5.1.5",
    "babel-loader": "^8.1.0",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "^4.3.0",
    "nodemon": "^2.0.3",
    "source-map-loader": "^0.2.4",
    "ts-loader": "^7.0.4",
    "ts-node": "^8.10.1",
    "tsc": "^1.20150623.0",
    "tslint": "^6.1.2",
    "typescript": "^3.9.2",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11"
  },
  "engines": {
    "node": "13.11.0"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
